{
  "name": "@ethdebug/pointers",
  "version": "0.1.0-0",
  "description": "Reference implementation for ethdebug/format pointers",
  "main": "dist/src/index.js",
  "type": "module",
  "license": "MIT",
  "imports": {
    "#cursor": {
      "types": "./src/cursor.ts",
      "default": "./dist/src/cursor.js"
    },
    "#data": {
      "types": "./src/data.ts",
      "default": "./dist/src/data.js"
    },
    "#evaluate": {
      "types": "./src/evaluate.ts",
      "default": "./dist/src/evaluate.js"
    },
    "#machine": {
      "types": "./src/machine.ts",
      "default": "./dist/src/machine.js"
    },
    "#read": {
      "types": "./src/read.ts",
      "default": "./dist/src/read.js"
    },
    "#dereference": {
      "types": "./src/dereference/index.ts",
      "default": "./dist/src/dereference/index.js"
    },
    "#dereference/*": {
      "types": "./src/dereference/*.ts",
      "default": "./dist/src/dereference/*.js"
    },
    "#test/*": "./test/*.ts"
  },
  "scripts": {
    "run-example": "node ./dist/bin/run-example.js",
    "prepare": "tsc --build",
    "watch": "tsc --build --watch",
    "test": "vitest"
  },
  "devDependencies": {
    "@ethdebug/format": "^0.1.0-0",
    "chalk": "^5.6.2",
    "cli-highlight": "^2.1.11",
    "ganache": "7.9.x",
    "solc": "^0.8.33",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "ethereum-cryptography": "^2.2.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
